{
  "name": "native-extensions-samples",
  "version": "0.0.1",
  "scripts": {
    "sdk": "rm -rf ./node_modules/@upscale/web-storefront-sdk && npm cache clear --force && npm i @upscale/web-storefront-sdk",
    "demo": "npm run build demo-checkout-native-extension",
    "demo_pack": "cd dist/demo-checkout-native-extension && npm pack",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "preassemble": "rm -rf dist && ng build --configuration=production --project=$npm_config_project",
    "assemble": "mkdir $npm_config_project && cp -rf dist/$npm_config_project $npm_config_project && cp -r cf-buildpack/* dist && mv $npm_config_project/* dist/public/ && rm -rf $npm_config_project"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.0",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "^11.2.14",
    "@angular/forms": "~11.1.0",
    "@angular/google-maps": "^12.2.1",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/router": "~11.1.0",
    "npm": "^6.14.15",
    "postcss": "^8.3.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "ua-parser-js": "^0.7.28",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.4",
    "@angular/cli": "~11.1.1",
    "@angular/compiler-cli": "~11.1.0",
    "@caas/service-client-angular": "file:libs/caas-service-client-angular-0.8.212.tgz",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@upscale/web-storefront-sdk": "file:libs/upscale-web-storefront-sdk-0.0.1-BETA.11.tgz",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2"
  }
}
